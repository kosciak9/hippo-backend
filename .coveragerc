[run]
omit =
    hippo/config/*
    hippo/*/migrations/*
    hippo/*/test/*
    */__init__.py
    */wsgi.py
    */urls.py
    */admin.py
    */manage.py
    
[report]
exclude_lines =
    pragma: no cover
    
    # Don't complain about missing debug-only code:
    def __unicode__
    def __repr__
    if self\.debug
    
    # Don't complain if tests don't hit defensive assertion code:
    raise AssertionError
    raise NotImplementedError
    
    # Don't complain if non-runnable code isn't run:
    if 0:
    if __name__ == .__main__.:
    
omit =
    hippo/config/*
    hippo/*/migrations/*
    hippo/*/test/*
    */__init__.py
    */wsgi.py
    */urls.py
    */admin.py
    */manage.py
    
show_missing = True
